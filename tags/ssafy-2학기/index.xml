    <item>
      <title>2학기 시작</title>
      <link>https://gudongyeop.github.io/%EB%8B%A8%EC%88%9C%EA%B8%B0%EB%A1%9D/2%ED%95%99%EA%B8%B0-%EC%8B%9C%EC%9E%91.html</link>
